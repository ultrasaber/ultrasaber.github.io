<sburb description='dickle'>

<classes>
	<animation class='dickle-dialog' frameInterval='6' x='-180' y='-524'/>
	
	<character class='dickle' name='dickle' sx='-29' sy='-48' sWidth='58' sHeight='96' width='30' height='30' sheet='dickleSheet' state='walkFront' facing='Front'></character>
	
	<action class='beDickle' name='Be Dickle.' command='macro'>
		<args>
			<action command='changeChar' noDelay='true'>
				<args>dickle</args>
				<action command='playSong' noDelay='true'>
					<args>dickleSong,0</args>
				</action>
			</action>
		</args>
	</action>
</classes>

<assets>
	<asset name='dickleSheet' type='graphic'>chars/dickle/dickle.png</asset>
	<asset name='dickleSong' type='audio'>audio/music/grudge.mp3</asset>
	<asset name='dickle_talk' type='graphic'>dialogs/karkat/karkat_talk.png</asset>
</assets>

<hud>
	<dialogsprites>	
  		<animation name='dickle_talk' class='dickle-dialog' sheet='dickle_talk' length='2'/>
  </dialogsprites>
</hud>
</sburb>
